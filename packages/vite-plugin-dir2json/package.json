{
  "name": "vite-plugin-dir2json",
  "version": "1.0.3",
  "description": "Convert the directory structure into json data containing supported file paths",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "files": [
    "*.d.ts",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "dir2json",
    "simple",
    "json"
  ],
  "author": "buddywang",
  "repository": "https://github.com/buddywang/vite-plugin-dir2json",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.11",
    "local-pkg": "^0.5.0",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "vite": ">=2.x"
  }
}